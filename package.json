{
  "name": "wetchn",
  "version": "0.0.7",
  "description": "Worker-Fetchication - A fetch library that behave like Next.js`s fetch for Cloudflare Workers.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yieldspace/wetchn"
  },
  "keywords": [
    "fetch",
    "typescript",
    "cloudflare",
    "cloudflare workers",
    "http",
    "edge",
    "serverless"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "sizumita <contact@yieldspace.app>",
  "scripts": {
    "build": "rimraf dist && run-p build:esbuild build:types",
    "build:types": "tsc -p tsconfig.build.json",
    "build:esbuild": "node build.js",
    "release": "yarn eslint . && yarn build && yarn test && changeset version && changeset publish",
    "test": "vitest run",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@cloudflare/workers-types": "^4.20230518.0",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.43.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "glob": "^10.2.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "tsc": "^2.0.4",
    "typescript": "^5.1.3",
    "vitest": "^0.32.0",
    "vitest-environment-miniflare": "^2.14.0"
  },
  "packageManager": "yarn@3.6.0"
}
